{
  "expo": {
    "name": "Mobile App 112",
    "slug": "mockup-112-v02",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/Logo-white.png",
    "splash": {
      "image": "./assets/Logo-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Prototype Mobile application for 112 Emergency Services in Indonesia.\nDeveloped for PT ADP.\n"
  }
}
